<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <System.Windows.Forms.ApplicationConfigurationSection>
  <configSections>
      <appSettings>
        <!-- *****************************************************************************************************************-->
        <!-- *********************************** DATABASE FILE PATHS  ******************************************************-->
        <!-- *****************************************************************************************************************-->
        <add key="SQLite" value="data\database\sqlite\datamodels\Data.db" />
        <add key="SqlCe" value="data\database\sqlce\datamodels\Data.sdf" />
        <add key="SqlServer" value="data\database\mssql\datamodels\Data.mdf" />
        <add key="Access" value="data\database\access\datamodels\Data.accdb" />
        <add key="OleDb" value="data\database\oledb\datamodels\Data.mdb" />
        <add key="Excel" value="{FilePath}" />
        <add key="CSV" value="{FilePath}" />
        <!-- *****************************************************************************************************************-->
        <!-- ************************************** DOCUMENT FILE PATHS ******************************************************-->
        <!-- *****************************************************************************************************************-->
        <add key="Budget" value="ninja\template\Control.xlsx" />
        <add key="Report" value="ninja\template\Report.xlsx" />
        <add key="SavePath" value="res\template\report" />
