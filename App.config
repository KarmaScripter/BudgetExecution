        <!-- ***********************************  IMAGE RESOURCES     ********************************************************-->
        <!-- *****************************************************************************************************************-->
        <add key="DatabaseImages" value="badger\icons\database" />
        <add key="BadgerImages" value="badger\icons\badger\png" />
        <add key="DataImages" value="badger\icons\Data" />
        <add key="CloseBoxImages" value="badger\icons\close" />
