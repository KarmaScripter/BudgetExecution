        <add key="EmailImages" value="badger\icons\email\png" />
        <add key="EmployeeImages" value="badger\icons\employee\png" />
        <add key="ExecutionImages" value="badger\icons\execution\png" />
        <add key="FileImages" value="badger\icons\file\png" />
        <add key="FilterImages" value="badger\icons\filter\png" />
