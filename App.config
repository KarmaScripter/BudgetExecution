        <add key="SummaryImages" value="badger\icons\summary\png" />
        <add key="ToolBarImages" value="badger\icons\toolbar\png" />
        <add key="ClientSettingsProvider.ServiceUri" value="" />
      <connectionStrings>
        <!-- *****************************************************************************************************************-->
        <!-- ***********************************   CONNECTION STRINGS   ******************************************************-->
        <!-- *****************************************************************************************************************-->
        <add name="SQLite" providerName="System.Data.SQLite" connectionString="Data Source=data\database\sqlite\datamodels\Data.db" />
        <add name="SqlServer" providerName="System.Data.SqlClient" connectionString="Data Source=(LocalDB)\MSSQLLocalDB;AttachDbFilename=data\database\mssql\datamodels\Data.mdf;Integrated Security=True;Connect Timeout=30" />
        <add name="SqlCe" providerName="System.Data.SqlServerCe" connectionString="Data Source=data\database\sqlce\datamodels\Data.sdf" />
        <add name="Excel" connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source={FilePath};Extended Properties='Excel 12.0;HDR=YES;IMEX=1;'" providerName="System.Data.OleDb" />
        <add name="Access" connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source=data\database\access\datamodels\Data.accdb;Persist Security Info=False" providerName="System.Data.OleDb" />
        <add name="OleDb" connectionString="Provider=Microsoft.ACE.OLEDB.12.0;Data Source={FilePath};Extended Properties='Excel 12.0;HDR=YES;IMEX=1;'" providerName="System.Data.OleDb" />
        <add name="CSV" connectionString="Provider=Microsoft.Jet.OleDb.4.0;Data Source={FilePath};Extended Properties='Text';FMT='Delimited'" providerName="System.Data.OleDb" />
        <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|" />
        <add name="DefaultConnection1" connectionString="Data Source = |SQL/CE|" />
      </connectionStrings>
      </appSettings>
  </configSections>
  </System.Windows.Forms.ApplicationConfigurationSection>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-12.0.0.0" newVersion="12.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>